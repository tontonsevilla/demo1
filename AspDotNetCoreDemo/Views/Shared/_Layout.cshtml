<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />

    <link rel="icon" type="image/x-icon" href="favicon.ico">

    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.11.2/css/all.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap">
    <link rel="stylesheet" href="@Url.Content("~/mdbootstrap/css/bootstrap.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/mdbootstrap/css/mdb.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/mdbootstrap/css/style.css")">

    <link rel="stylesheet" href="css/style.css" />

    <title>AspDotNetCoreDemo @(string.Format("{0}", string.IsNullOrEmpty(ViewBag.Title) ? string.Empty : $"- {ViewBag.Title}"))</title>
</head>
<body>
    <!--Navbar-->
    <nav class="navbar navbar-expand-lg navbar-dark special-color-dark">

        <!-- Navbar brand -->
        <a class="navbar-brand" href="/">AspDotNetCore Demo</a>

    </nav>
    <!--/.Navbar-->

    <div style="min-height: 89vh;">
        @RenderBody()
    </div>

    <!-- Footer -->
    <footer class="page-footer font-small special-color">

        <!-- Copyright -->
        <div class="footer-copyright text-center py-3">
            @DateTime.Now.Year © AspDotNetCore Demo. Powered by
            <a href="https://mdbootstrap.com/education/angular/"> MDBootstrap.com</a>
        </div>
        <!-- Copyright -->

    </footer>
    <!-- Footer -->

    <script type="text/javascript" src="@Url.Content("~/mdbootstrap/js/jquery.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/mdbootstrap/js/popper.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/mdbootstrap/js/bootstrap.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/mdbootstrap/js/mdb.min.js")"></script>
</body>
</html>
